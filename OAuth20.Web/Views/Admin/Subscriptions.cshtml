<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
@using OAuth20.Web.Models
@model OAuth20.Web.Models.SubscriptionsResponse
@{
    var subscriptions = Model.Subscriptions;
    if (subscriptions!=null && subscriptions.Any())
    {
        <form action="/admin/message" method="post">
            <span>Message content: </span><input type="text" name="Content"/>
            <input type="Submit" value="Send" id="btnSend" />
        </form>    

        <b>Current subscriptions</b>
        <ul>
        @foreach (var s in subscriptions)
            {<li>@s.Value - <a class="btn btn-danger" href="/admin/revoke/@s.Key">Revoke</a></li>}
        </ul>
    }
    else
    {
        <div>No subscriptions.</div>
    }
}
<br />
Go back to <a href="/user/profile">profile</a> page.

  
   